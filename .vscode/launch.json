{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "NestJS Debug",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "port": 9229,
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
